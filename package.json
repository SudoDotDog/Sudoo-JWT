{
	"name": "@sudoo/jwt",
	"version": "3.5.0",
	"description": "JWT generator verifier for Node",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SudoDotDog/Sudoo-JWT.git"
	},
	"keywords": [
		"JWT"
	],
	"author": "WMXPY",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SudoDotDog/Sudoo-JWT/issues"
	},
	"homepage": "https://github.com/SudoDotDog/Sudoo-JWT#readme",
	"dependencies": {
		"@sudoo/jwt-config": "^1.3.0"
	},
	"peerDependencies": {
		"@sudoo/token": "*"
	},
	"devDependencies": {
		"@sudoo/token": "^1.6.1",
		"@sudoo/triforce": "^3.10.1"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"script/**/*.ts",
			"src/**/*.ts"
		],
		"instrument": true,
		"reporter": [
			"json",
			"text-summary"
		],
		"report-dir": "coverage",
		"sourceMap": true
	}
}