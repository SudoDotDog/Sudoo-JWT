{
	"name": "@sudoo/jwt",
	"version": "2.0.0",
	"description": ":clap: JWT generator verifier for Node",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SudoDotDog/Sudoo-JWT.git"
	},
	"keywords": [
		"JWT"
	],
	"author": "WMXPY",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/SudoDotDog/Sudoo-JWT/issues"
	},
	"homepage": "https://github.com/SudoDotDog/Sudoo-JWT#readme",
	"peerDependencies": {
		"@sudoo/token": "*"
	},
	"dependencies": {
		"@sudoo/jwt-config": "latest"
	},
	"devDependencies": {
		"@sudoo/io": "^1.8.0",
		"@sudoo/token": "^1.6.0",
		"@sudoo/triforce": "^2.9.26"
	},
	"nyc": {
		"extension": [
			".ts"
		],
		"include": [
			"script/**/*.ts",
			"src/**/*.ts"
		],
		"instrument": true,
		"reporter": [
			"json",
			"text-summary"
		],
		"report-dir": "coverage",
		"sourceMap": true
	}
}